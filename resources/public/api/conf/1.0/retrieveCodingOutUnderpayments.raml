    body:
      application/json:
        type: !include schemas/retrieve_coding_out_underpayments_response.json
        examples:
          example-1:
            description: Example Response
            value: !include examples/retrieveCodingOutUnderpayments/retrieve_coding_out_underpayments_response.json
